@{
    ViewData["Title"] = "Resultado del Examen";
    var puntaje = TempData["Puntaje"];
    var diagnostico = TempData["Diagnostico"];
}

<h2>¡Has completado el examen!</h2>
<p><strong>Puntaje total:</strong> @puntaje</p>
<p><strong>Diagnóstico:</strong> @diagnostico</p>

<a asp-controller="Home" asp-action="Index" class="btn btn-primary">Regresar al inicio</a>
